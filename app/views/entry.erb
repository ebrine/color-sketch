<div class="container">
  <h2>Title: <%=@entry_title%></h2>
  <h3>Author: <%=@entry_author%></h3>
  <h4>Tags:  </h4>
  <%@entry_tags.each do |tag| %>
  <li><%=tag%></li>
  <%end%>
  <p> <%=@entry_body%></p>

  
  <form action="/edit?entry_id=<%=@entry_id%>" method="post" ><input type="submit" value="Edit"/></form>
  <form action="/delete?entry_id=<%=@entry_id%>" method="post" ><input type="submit" value="Delete"/></form>
  <a href="/">Back to home</a>
</div>
